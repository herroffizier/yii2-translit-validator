{
    "name": "herroffizier/yii2-translit-validator",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "keywords": ["yii", "yii2", "yii 2", "extension", "validator", "translit", "transliteration", "slug", "urlify"],
    "description": "Yii2 validator that transliterates model attribute values.",
    "require": {
        "yiisoft/yii2": "*",
        "jbroadway/urlify": "1.0.*@dev"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "codeception/codeception": "2.0.*",
        "codeception/verify": "*",
        "codeception/specify": "*",
        "squizlabs/php_codesniffer": "dev-master",
        "phing/phing": "*"
    },
    "autoload": {
        "psr-4": {"herroffizier\\yii2tv\\tests\\": "tests/", "herroffizier\\yii2tv\\": "src/"}
    }
}
